{
  "installCommand": "pnpm install --no-frozen-lockfile",
  "buildCommand": "pnpm run build",
  "devCommand": "pnpm run dev",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    },
    "app/api/**/*.tsx": {
      "maxDuration": 30
    }
  },
  "outputDirectory": ".next",
  "ignoreCommand": "pnpm lint",
  "regions": ["iad1"],
  "github": {
    "enabled": true
  }
}
